#prime number or not using for loop
number=int(input("Enter the number:"))
for i in range(2,number):
  if(number%i==0):
    print(number,"is not a prime number")
    break
else:
  print(number,"is a prime number")
#prime number or not using while loop
number=int(input("Enter the number:"))
i=2
while(i<number):
  if(number%i==0):
    print(number,"is not a prime number")
    break
  i=i+1
else:
  print(number,"is a prime number")
  i=i+1
